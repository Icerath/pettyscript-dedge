class Point(x, y) {
    fn __add__(self, other) {
        return Point(self.x + other.x, self.y + other.y);
    }
    fn __repr__(self) {
        return "Point { " + repr(self.x) +  ", " + repr(self.y) + " }";
    }
    fn __call__(self) {
        print(self);
    }
}
point = Point(1, 2) + Point(2, 3);
result = point();
print(result);