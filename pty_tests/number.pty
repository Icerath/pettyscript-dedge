assert = std.test.assert;
assert_eq = std.test.assert_eq;
assert_ne = std.test.assert_ne;


assert_eq(4 + 5, 9);
assert_eq(4 * 5, 20);
assert_eq(9 / 3, 3);
assert_eq(5 / 2, 2.5);
assert_eq(-12.abs(), 12);
assert_eq(15 % 3, 0);
assert_eq(13 % 4, 1);


assert(4 < 5);
assert(5 > 4);
assert(4 <= 5);
assert(5 >= 4);
assert(5 <= 5);
assert(5 >= 5);

assert_eq(repr(12), "12");
assert_eq(repr(-12), "-12");

assert_ne(1, 2);